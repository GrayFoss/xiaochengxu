.submit_main{
  background-color: #f4f4f4;
  padding-top: 12rpx;
}
.submit_list{
  box-sizing: border-box;
  width: 100%;
  height: 120rpx;
  background-color: #fff;
  display: flex;
  display: -webkit-flex;
  padding-left: 36rpx;
  padding-top: 36rpx;
  margin-bottom: 12rpx;
}
.hint{
  width: 12rpx;
  height: 50rpx;
  margin-right: 20rpx;
}
.submit_title{
  flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  -moz-flex: 1;
}
.submit_right{
   width: 120rpx;
   padding-right: 36rpx;
}
.submit_right image{
  width: 12rpx;
  margin-left: 15rpx;
  height: 24rpx;
}